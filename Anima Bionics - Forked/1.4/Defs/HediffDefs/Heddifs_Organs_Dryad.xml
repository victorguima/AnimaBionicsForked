<Defs>


 <HediffDef ParentName="ImplantHediffBase"> <!-- Anima Plates -->
    <defName>AnimaPlateHediff</defName>
    <label>wooden plates</label>
    <description>Wooden plates covering the entire body, quite pretty. Eases pain and helps at releasing neural heat, but is susceptible to fire.</description> <!-- Rebalance by Victor -->
    <labelNoun>a wooden plate</labelNoun>
    <defaultLabelColor>(0, 0.6, 0.4)</defaultLabelColor>
    <stages>
		<li>
			<painFactor>0.9</painFactor>
			<statOffsets>
				<ArmorRating_Blunt>0.10</ArmorRating_Blunt> 
				<ArmorRating_Heat>-0.25</ArmorRating_Heat> <!-- Rebalance by Victor -->
				<ArmorRating_Sharp>0.10</ArmorRating_Sharp>
				<PawnBeauty>1</PawnBeauty>
				<PsychicEntropyRecoveryRate>0.10</PsychicEntropyRecoveryRate> <!-- Rebalance by Victor -->
			</statOffsets>
		</li>
	</stages>
  </HediffDef>

  <ThingDef ParentName="AnimaImplant">
    <defName>AnimaPlate</defName>
    <label>wooden plating</label>
    <description>Wooden plates covering the entire body, quite pretty. Eases pain and helps at releasing neural heat, but is susceptible to fire.</description> <!-- Rebalance by Victor -->
    <costList>
      <MedicineHerbal>10</MedicineHerbal>
      <AnimaFiber>50</AnimaFiber>
      <WoodLog>500</WoodLog>
    </costList>
    <statBases>
      <Mass>2.5</Mass>
    </statBases>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallImplantBase">
    <defName>InstallAnimaPlate</defName>
    <label>install wooden plating</label>
    <description>Install a wooden plating.</description>
    <jobString>Installing wooden plating</jobString>
    <skillRequirements>
      <Medicine>10</Medicine>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>AnimaPlate</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>AnimaPlate</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Torso</li>
    </appliedOnFixedBodyParts>
    <addsHediff>AnimaPlateHediff</addsHediff>
  </RecipeDef>

  <RecipeDef ParentName="SurgeryRemoveImplantBase">
    <defName>removeAnimaPlating</defName>
    <label>remove wooden plating</label>
    <description>Remove wooden plating.</description>
    <jobString>Removing wooden plating.</jobString>
    <removesHediff>AnimaPlateHediff</removesHediff>
  </RecipeDef>
  
  
   <HediffDef ParentName="AddedBodyPartBase"> <!-- Anima Liver -->
    <defName>AnimaRootsliverHediff</defName>
    <label>anima roots (liver)</label>
    <description>Roots spreading through the body, this one goes into the liver. Boosts blood circulation and coagulation while strenghtening the immune system. Helps at containing neural heat, and allows the owner to meditate better.</description> <!-- Rebalance by Victor -->
    <labelNoun>an anima roots liver</labelNoun> <!-- Rebalance by Victor -->
    <defaultLabelColor>(0, 0.6, 0.4)</defaultLabelColor>
	<stages>
		<li>
			<statOffsets>
				<ImmunityGainSpeedFactor>0.1</ImmunityGainSpeedFactor>
				<MeditationFocusGain>0.05</MeditationFocusGain>
				<PsychicEntropyMax>5</PsychicEntropyMax>
			</statOffsets>
			<capMods>
				<li>
					<capacity>BloodPumping</capacity>
					<offset>0.05</offset>
				</li>
			</capMods>
		</li>
	</stages>
	<addedPartProps>
		<solid>true</solid>
		<partEfficiency>1</partEfficiency> <!-- Rebalance by Victor -->
		<betterThanNatural>true</betterThanNatural>
	</addedPartProps>
    <spawnThingOnRemoved>AnimaRootsliver</spawnThingOnRemoved>
  </HediffDef>

