<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- All of my changes have a comment "Rebalance by Victor" -->
	<HediffDef ParentName="AddedBodyPartBase"> <!-- Anima Leg -->
		<defName>AnimaWoodLeg</defName>
		<label>Anima leg</label>
		<description>This leg has been replaced with wood, reminiscent to that of anima trees. Helps at releasing neural heat, and allows the owner to meditate better.</description> <!-- Rebalance by Victor -->
		<labelNoun>an anima leg</labelNoun>
		<defaultLabelColor>(0, 0.6, 0.4)</defaultLabelColor>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1</partEfficiency> <!-- Rebalance by Victor -->
			<betterThanNatural>true</betterThanNatural>
		</addedPartProps>
		<stages> <!-- Rebalance by Victor -->
			<li>
				<statOffsets>
					<MeditationFocusGain>0.10</MeditationFocusGain>
					<PsychicEntropyRecoveryRate>0.10</PsychicEntropyRecoveryRate>
				</statOffsets>
			</li>
		</stages>
		<spawnThingOnRemoved>Anima_WoodLeg</spawnThingOnRemoved>
	</HediffDef>

  <ThingDef ParentName="AnimaPart">
    <defName>Anima_WoodLeg</defName>
    <label>Anima leg</label>
	<description>This leg has been replaced with wood, reminiscent to that of anima trees. Helps at releasing neural heat, and allows the owner to meditate better.</description> <!-- Rebalance by Victor -->
    <costList>
      <MedicineHerbal>10</MedicineHerbal>
      <AnimaFiber>200</AnimaFiber>
      <WoodLog>300</WoodLog>
    </costList>
    <statBases>
      <Mass>1.5</Mass>
      <MarketValue>500</MarketValue>
	   <WorkToMake>3000</WorkToMake>
    </statBases>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
    <defName>InstallAnimaLeg</defName>
    <label>install anima leg</label>
    <description>Install an anima leg.</description>
    <jobString>Installing an anima leg.</jobString>
    <skillRequirements>
      <Medicine>10</Medicine>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Anima_WoodLeg</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Anima_WoodLeg</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Leg</li>
    </appliedOnFixedBodyParts>
    <addsHediff>AnimaWoodLeg</addsHediff>
  </RecipeDef>

  <RecipeDef ParentName="SurgeryRemoveImplantBase">
    <defName>removeanimaleg</defName>
    <label>remove anima leg</label>
    <description>Remove anima leg.</description>
    <jobString>Removing anima leg.</jobString>
    <removesHediff>AnimaWoodLeg</removesHediff>
  </RecipeDef>

  <HediffDef ParentName="AddedBodyPartBase"> <!-- Anima War Arm -->
    <defName>AnimaWarArmHeddiff</defName>
    <label>Anima war arm</label>
    <description>A wooden war anima arm, consisting of multiple very sharp sticks that are stronger than they look. Helps at releasing neural heat, and allows the owner to meditate better.</description> <!-- Rebalance by Victor -->
    <labelNoun>an outer blade</labelNoun>
    <defaultLabelColor>(0, 0.6, 0.4)</defaultLabelColor>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>Scythe</label>
            <capacities>
              <li>Stab</li>
            </capacities>
            <power>25.0</power>
            <cooldownTime>0.8</cooldownTime>
            <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
          </li>
        </tools>
      </li>
    </comps>
    <stages>
      <li>
        <statOffsets>
          <MeleeHitChance>+2</MeleeHitChance>
          <SharpDamageMultiplier>1.25</SharpDamageMultiplier>
		  <MeditationFocusGain>0.05</MeditationFocusGain> <!-- Rebalance by Victor -->
		  <PsychicEntropyRecoveryRate>0.20</PsychicEntropyRecoveryRate> <!-- Rebalance by Victor -->
        </statOffsets>
      </li>
    </stages>
    <spawnThingOnRemoved>AnimaWar_Arm</spawnThingOnRemoved>
    <addedPartProps>
      <isGoodWeapon>true</isGoodWeapon>
         <partEfficiency>1.0</partEfficiency>
    </addedPartProps>
  </HediffDef>

  <ThingDef ParentName="AnimaPart">
    <defName>AnimaWar_Arm</defName>
    <label>Anima war arm</label>
	<description>A wooden war anima arm, consisting of multiple very sharp sticks that are stronger than they look. Helps at releasing neural heat, and allows the owner to meditate better.</description> <!-- Rebalance by Victor -->
    <costList>
      <MedicineHerbal>10</MedicineHerbal>
      <AnimaFiber>300</AnimaFiber>
      <WoodLog>300</WoodLog>
    </costList>
    <statBases>
      <Mass>4</Mass>
      <MarketValue>1000</MarketValue>
	   <WorkToMake>4000</WorkToMake>
    </statBases>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
    <defName>installanimawararm</defName>
    <label>install an anima war arm</label>
    <description>Install anima war arm.</description>
    <jobString>Installing anima war arm.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>AnimaWar_Arm</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>AnimaWar_Arm</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Arm</li>
    </appliedOnFixedBodyParts>
    <addsHediff>AnimaWarArmHeddiff</addsHediff>
    <skillRequirements>
      <Medicine>10</Medicine>
    </skillRequirements>
  </RecipeDef>

	<ThingDef ParentName="AnimaPart">
      <defName>animaentArm</defName>
      <label>Anima ent arm</label>
      <description>An arm perfect for being in sync with nature.</description>
    <costList>
      <MedicineHerbal>10</MedicineHerbal>
      <AnimaFiber>300</AnimaFiber>
      <WoodLog>300</WoodLog>
    </costList>
         <statBases>
      <Mass>5</Mass>
      <MarketValue>500</MarketValue>
	   <WorkToMake>3000</WorkToMake>
    </statBases>
   </ThingDef>

	<HediffDef ParentName="AddedBodyPartBase"> <!-- Anima Ent Arm -->
		<defName>anima_entArm_heddiff</defName> <!-- Rebalance by Victor -->
		<label>Anima ent arm</label>
		<labelNoun>anima ent arm</labelNoun>
		<description>A wooden ent arm. The anima bark is fine-tuned to the planet's flora, helping the owner in plant related tasks. Helps at releasing neural heat, and allows the owner to meditate better.</description>
		<spawnThingOnRemoved>animaentArm</spawnThingOnRemoved>
		<defaultLabelColor>(0, 0.6, 0.4)</defaultLabelColor>
		<stages>
			<li>
				<statOffsets>
					<PlantHarvestYield>0.1</PlantHarvestYield>
					<PlantWorkSpeed>0.1</PlantWorkSpeed>
					<MeditationFocusGain>0.10</MeditationFocusGain> <!-- Rebalance by Victor -->
					<PsychicEntropyRecoveryRate>0.10</PsychicEntropyRecoveryRate> <!-- Rebalance by Victor -->
				</statOffsets>
			</li>
		</stages>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1</partEfficiency> <!-- Rebalance by Victor -->
			<betterThanNatural>true</betterThanNatural>
		</addedPartProps>
	</HediffDef>
   
   <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
      <defName>Installanimaentarm</defName>
      <label>install anima ent arm</label>
      <description>Installs wooden ent arm.</description>
      <jobString>Installing anima ent arm.</jobString>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>animaentArm</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <thingDefs>
            <li>animaentArm</li>
         </thingDefs>
      </fixedIngredientFilter>
      <appliedOnFixedBodyParts>
         <li>Shoulder</li>
      </appliedOnFixedBodyParts>
      <addsHediff>anima_entArm_heddiff</addsHediff> <!-- Rebalance by Victor -->
   </RecipeDef>

  <HediffDef ParentName="AddedBodyPartBase"> <!-- Anima Heart -->
    <defName>aniheart_heddiff</defName> <!-- Rebalance by Victor -->
    <label>aniheart</label>
    <description>A heart made out of multiple dryads, which helps the user's blood circulation and closes wounds quickier. Expands and strenghtens the owner's psychic abilities.</description> <!-- Rebalance by Victor -->
    <labelNoun>an aniheart</labelNoun> <!-- Rebalance by Victor -->
    <defaultLabelColor>(0, 0.6, 0.4)</defaultLabelColor>
	<stages>
		<li>
			<statOffsets> <!-- Rebalance by Victor -->
				<PsychicSensitivity>0.10</PsychicSensitivity>
				<PsychicEntropyMax>15</PsychicEntropyMax>
			</statOffsets>
			<capMods>
				<li>
					<capacity>BloodPumping</capacity> <!-- Rebalance by Victor -->
					<offset>0.10</offset>
				</li>
			</capMods>
			<totalBleedFactor>0.9</totalBleedFactor> <!-- Rebalance by Victor -->
		</li>
	</stages>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1</partEfficiency> <!-- Rebalance by Victor -->
      <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
    <spawnThingOnRemoved>aniheart</spawnThingOnRemoved>
  </HediffDef>

  <ThingDef ParentName="AnimaPart">
    <defName>aniheart</defName>
    <label>Aniheart</label>
