	<costList>
      <MedicineHerbal>20</MedicineHerbal>
      <AnimaFiber>500</AnimaFiber>
      <WoodLog>100</WoodLog>
    </costList>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
    <defName>installaniheart</defName>
    <label>install aniheart</label>
    <description>Install aniheart.</description>
    <jobString>Installing aniheart.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>aniheart</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>aniheart</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Heart</li>
    </appliedOnFixedBodyParts>
    <addsHediff>aniheart_heddiff</addsHediff> <!-- Rebalance by Victor -->
    <skillRequirements>
      <Medicine>10</Medicine>
    </skillRequirements>
  </RecipeDef>
</Defs>
